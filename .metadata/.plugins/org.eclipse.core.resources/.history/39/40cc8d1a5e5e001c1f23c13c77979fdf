package com.uni.chap01_Abstraction.model.vo;

public class Student {
	//vo : value object:  테이블 컴럼, 데이터등을 전달하고 관리하는 value object
	//테이블 컬럼을 모아놓은 단위 ,데이터 전달및 표현관리
	/*
	 * 클래스의 구조 
	 * 
	 * public class 클래스명{
	 * 
	 * 		//필드부 
	 * 
	 * 		//생성자부
	 * 
	 * 		//메소드부
	 * 
	 * }
	 * 
	 * */

	public String name;
	public int age;
	public double height;
	public int kor;
	public int math;

}
